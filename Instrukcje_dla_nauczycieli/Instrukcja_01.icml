  <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Code">
    <Content>OVA</Content>
  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="$ID/NormalCharacterStyle">
    <Content> w VirtualBox wszystko już jest zainstalowane, uczniowie tylko zakładają sobie konta na GitHub. Tę czynność w ogóle można zlecić do wykonania samodzielnie w domu </Content>
  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Italic">
    <Content>przed zajęciami</Content>
  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="$ID/NormalCharacterStyle">
    <Content>.</Content>
  </CharacterStyleRange>
